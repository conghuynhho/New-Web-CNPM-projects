@model IEnumerable<WebCNPM.Models.DOIBONG>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SAN.TenSan)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenDoi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NgayThanhLap)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SAN.TenSan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenDoi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NgayThanhLap)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MaDoi }) |
            @Html.ActionLink("Details", "Details", new { id=item.MaDoi }) |
